<p id="notice"><%= notice %></p>

<p>Hello <%= current_user.email %>!</p>
<h1>Listing Corgis</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Age</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @corgis.each do |corgi| %>
      <tr>
        <td><%= corgi.name %></td>
        <td><%= corgi.age %></td>
        <td><%= corgi.user %></td>
        <td><%= link_to 'Show', corgi %></td>
        <td><%= link_to 'Edit', edit_corgi_path(corgi) %></td>
        <td><%= link_to 'Destroy', corgi, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Corgi', new_corgi_path %>
<%= link_to 'Log Out', destroy_user_session_path, method: :delete %>
